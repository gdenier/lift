{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "umd/**", "build/**", ".next"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "outputs": ["dist/**", "umd/**", "build/**", ".next"]
    }
  }
}
